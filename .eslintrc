{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "airbnb-base",
      "plugin:vue/recommended"
    ],
    "rules": {
      "import/no-unresolved": "off",
      "import/prefer-default-export": "off",
      "no-underscore-dangle": "off",
      "no-param-reassign": "off"
    }
  }
  